			<groupId>org.eclipse.daanse</groupId>
			<artifactId>org.eclipse.daanse.ws.runtime.registrar</artifactId>
			<version>${revision}</version>
			<scope>runtime</scope>
		</dependency>
		<dependency>
			<groupId>org.eclipse.daanse</groupId>
